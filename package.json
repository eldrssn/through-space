{
  "name": "imedia-gigachat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build ",
    "build:testing": "tsc && vite build --base=./ --mode development",
    "build:production": "tsc && vite build --base=https://sp.imweb.ru/techinsider-planet/ --mode prod",
    "preview": "vite preview",
    "generate": "plop",
    "swagger:fetch": "set -a && . .env && set +a && curl -u \"$VITE_OPENAPI_USER:$VITE_OPENAPI_PASS\" \"$VITE_OPENAPI_URL\" -o ./api-docs.json",
    "swagger:generate": "rimraf ./src/api/generated && openapi-generator-cli generate",
    "apigen": "npm run swagger:fetch && npm run swagger:generate",
    "lint:check": "eslint --cache .",
    "lint": "eslint --cache . --fix",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx,js,json}\"",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,js,json}\"",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run prettier"
    ]
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.18.4",
    "@pixi/react": "^7.1.2",
    "@tanstack/react-query": "^5.72.2",
    "@vitejs/plugin-legacy": "^6.0.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "glob": "^11.0.1",
    "gsap": "^3.12.7",
    "pixi.js": "^7.4.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-transition-group": "^4.4.5",
    "styled-components": "^6.1.16",
    "use-clipboard-copy": "^0.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-basic-ssl": "^2.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "plop": "^4.0.1",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
